{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "astro",
  "main": "./dist/_worker.js/index.js",
  "compatibility_date": "2025-04-15",
  "compatibility_flags": ["nodejs_compat"],
  "assets": {
    "binding": "ASSETS",
    "directory": "./dist"
  },
  "observability": {
    "enabled": true
  },
  "vars": {
    "WEBFLOW_API_TOKEN": "",
    "MONUMENTS_COLLECTION_ID": "",
    "ECOSYSTEM_COLLECTION_ID": "",
    "WEBFLOW_SITE_ID": ""
  }
}
